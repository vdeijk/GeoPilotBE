{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=../BE.Data/Data/geodata.db"
  },
  "DatabaseProvider": "SQLite",
  "AllowedHosts": "*",
  "EnableHttpsRedirection": true
  ,
  "Cors": {
    "AllowAngularApp": {
      "Origins": ["http://localhost:4200", "https://localhost:4200"],
      "AllowAnyMethod": true,
      "AllowAnyHeader": true,
      "AllowCredentials": true
    }
  }
}
